{
    "name": "deinname/laravel-voyager-to-filament",
    "description": "Ein Laravel-Plugin, um Voyager-Modelle in Filament-Resources zu konvertieren.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dein Name",
            "email": "dein.email@example.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0",
        "doctrine/dbal": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "VoyagerToFilament\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VoyagerToFilament\\VoyagerToFilamentServiceProvider"
            ]
        }
    }
}