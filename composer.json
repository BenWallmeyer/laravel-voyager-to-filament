{
    "name": "benwallmeyer/laravel-voyager-to-filament",
    "description": "A laravel Plugin that converts Laravel Voyager Models to Filament",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Wallmeyer",
            "email": "bw@coswa.it"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "doctrine/dbal": "^3.0",
        "kitloong/laravel-migrations-generator": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "VoyagerToFilament\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VoyagerToFilament\\VoyagerToFilamentServiceProvider"
            ]
        }
    }
}
